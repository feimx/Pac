{
    "name": "feimx/pac",
    "description": "This Package allow to conect multiple Pac providers sistem",
    "keywords": [
        "feimx",
        "Pac",
        "cfdi",
        "facturacion electronica"
    ],
    "homepage": "https://github.com/feimx/pac",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Andrade",
            "email": "j.andrade.dev@gmail.com",
            "homepage": "https://fei.com.mx",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.6",
        "illuminate/validation": "~5.6.0",
        "meng-tian/async-soap-guzzle": "^0.2.4",
        "nesbot/carbon": "^1.20"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "FeiMx\\Pac\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FeiMx\\Pac\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FeiMx\\Pac\\PacServiceProvider"
            ],
            "aliases": {
                "Pac": "FeiMx\\Pac\\Pac"
            }
        }
    }
}
